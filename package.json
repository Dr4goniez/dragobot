{
  "name": "dragobot",
  "version": "7.0.0",
  "description": "A Wikipedia bot.",
  "main": "dist/bot.js",
  "scripts": {
    "start": "node -e \"const date = new Date().toISOString().replace(/\\.\\d+Z$/, '').replace(/[-T:]/g, ''); require('child_process').execSync(`node dist/bot.js > logs/${date}.txt 2>&1`);\"",
    "start-node": "ts-node src/bot.ts",
    "test": "ts-node src/test.ts",
    "test-log": "node -e \"const date = new Date().toISOString().replace(/\\.\\d+Z$/, '').replace(/[-T:]/g, ''); require('child_process').execSync(`ts-node src/test.ts > logs/${date}_test.txt 2>&1`);\"",
    "prebuild": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "npm run prebuild & tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dr4goniez/dragobot.git"
  },
  "author": "Dr4goniez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dr4goniez/dragobot/issues"
  },
  "homepage": "https://github.com/Dr4goniez/dragobot#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "ip-wiki": "^1.1.1",
    "mwbot-ts": "github:Dr4goniez/mwbot-ts#release"
  }
}
